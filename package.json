    "submit_checkin": "ts-node movectf_checkin/submit.ts",
    "publish_answer1": "sui client publish movectf_answer1 --gas-budget 30000 --skip-dependency-verification --skip-fetch-latest-git-deps",
    "submit_answer1": "ts-node movectf_answer1/submit.ts",
    "publish_answer2": "sui client publish movectf_answer2 --gas-budget 30000 --skip-dependency-verification --skip-fetch-latest-git-deps",
    "submit_answer2": "ts-node movectf_answer2/submit.ts",
    "publish_answer3": "sui client publish movectf_answer3 --gas-budget 30000 --skip-dependency-verification --skip-fetch-latest-git-deps",
    "submit_answer3": "ts-node movectf_answer3/submit.ts",
    "list": "sui client objects --address 0x8004e754d011ad38d41d7548f5faaaf3789c1838",
    "faucet": "ts-node typescript/faucet.ts",
    "balance": "ts-node typescript/balance.ts"
  },
